{
    "name": "Clipboard Customizer",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "This is a Chrome extension. You can custom original clipboard data.",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "contextMenus"
    ]
}